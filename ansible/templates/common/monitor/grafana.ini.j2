[server]
http_port = 3000
domain = localhost
root_url = %(protocol)s://%(domain)s:%(http_port)s/

[security]
admin_user = admin
admin_password = {{ grafana_admin_password | default('admin') }}

[auth.anonymous]
enabled = false

[analytics]
reporting_enabled = false
check_for_updates = true

[log]
mode = console file
level = info 